<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mydns.sys1yagi.android.printingframeworksample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="jp.mydns.sys1yagi.android.printingframeworksample.PrintingFrameworkSampleActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.mydns.sys1yagi.android.printingframeworksample.image.ImagePrintActivity"
            android:label="@string/title_activity_image_print" >
        </activity>
        <activity
            android:name="jp.mydns.sys1yagi.android.printingframeworksample.html.HtmlPrintActivity"
            android:label="@string/title_activity_html_print" >
        </activity>
        <activity
            android:name="jp.mydns.sys1yagi.android.printingframeworksample.custom.CustomPrintActivity"
            android:label="@string/title_activity_custom_print" >
        </activity>
    </application>

</manifest>
